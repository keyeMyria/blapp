{
  "compilerOptions": {
    "outDir": "./blapp/frontend/static/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es2015",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "pretty": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "blapp/*": ["blapp/frontend/src/*"],
    },
  },
}
